{
  "name": "pnpm-turbo-monorepo-template",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "jest"
  },
  "devDependencies": {
    "@icebreakers/eslint-config-ts": "^1.0.2",
    "@types/jest": "^29.2.3",
    "antlr4": "^4.11.0",
    "chevrotain": "^10.4.2",
    "eslint": "^8.29.0",
    "jest": "^29.3.1",
    "prettier": "latest",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "turbo": "latest",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.14.1"
}
